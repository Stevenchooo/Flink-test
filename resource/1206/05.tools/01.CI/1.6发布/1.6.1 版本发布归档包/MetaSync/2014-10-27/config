mysql.driver=com.mysql.jdbc.Driver
##warnning：source.user allows to have only query privilege!
##警告：source.user仅允许有查询权限，否则如果源和目标交换会导致严重危险
source.url=jdbc:mysql://192.168.8.242:3306/hive_test_src
source.user=root
source.pw=6EC0F2176B23266F3F998D7C46BB5BE4

dest.url=jdbc:mysql://192.168.8.242:3306/hive_test_dst
dest.user=root
dest.pw=6EC0F2176B23266F3F998D7C46BB5BE4

hdfs.root.path=hdfs://PlatB2slot2:9000/$OS_USER/data/
