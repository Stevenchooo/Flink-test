<reportSuite>
    <reports>
        <report tableName = "t_video_hvs_hours_watch_d" 
				keyFields = "projectid,activitychannelid,contentid,dtime" 
				period = "daily" timeFormat = "yyyyMMdd" timeField = "dtime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
				<field name="activitychannelid" regular="[0-9a-zA-Z./-]*"/>
				<field name="contentid" regular="[0-9a-zA-Z./-]*"/>
                <field name="dtime"  regular="[0-9a-zA-Z./-]*"/>
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
						video.hvs.hours.watch.vodwatchall.d,
						video.hvs.hours.watch.ltvwatchall.d
                    </reportIndex>
                </dimension>
            </dimensions>
        </report>
    </reports>
</reportSuite>
