<reportSuite>
    <reports>
        <report tableName = "t_video_users_hours_action_h" 
				keyFields = "projectid,activitychannelid,contentid,htime" 
				period = "hourly" timeFormat = "yyyyMMddHH" timeField = "htime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
				<field name="activitychannelid" regular="[0-9a-zA-Z./-]*"/>
				<field name="contentid" regular="[0-9a-zA-Z./-]*"/>
                <field name="htime"  regular="[0-9a-zA-Z./-]*"/>
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
						video.users.hours.action.hvswatch.h,
						video.users.hours.action.orderproduct.h
                    </reportIndex>
                </dimension>
            </dimensions>
        </report>
    </reports>
</reportSuite>
