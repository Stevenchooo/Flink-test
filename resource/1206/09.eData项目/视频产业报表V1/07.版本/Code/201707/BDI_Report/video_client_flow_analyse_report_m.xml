<reportSuite>
    <reports>
        <report tableName = "t_video_client_flow_analyse_m" 
				keyFields = "projectid,networktype,platformid,mtime" 
				period = "monthly" timeFormat = "yyyyMM" timeField = "mtime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
				<field name="networktype" regular="[0-9a-zA-Z./-]*"/>
				<field name="platformid" regular="[0-9a-zA-Z./-]*"/>
                <field name="mtime"  regular="[0-9a-zA-Z./-]*"/>
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
						video.client.flow.analyse.columns.m
                    </reportIndex>
                </dimension>
                <dimension id = "0" >
                    <reportIndex>
						video.client.flow.analyse.douflow.m
                    </reportIndex>
                </dimension>
            </dimensions>
        </report>
    </reports>
</reportSuite>
