<reportSuite>
    <reports>
        <report tableName = "t_video_internation_dashboard_pid_dm" 
				keyFields = "projectid,dtime" 
				period = "daily" timeFormat = "yyyyMMdd" timeField = "dtime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
                <field name="dtime"  regular="[0-9a-zA-Z./-]*"/>				
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
						video.internation.dashboard.pid.income.dm,
						video.internation.dashboard.pid.activeindex.dm,
						video.internation.dashboard.pid.activeuserindex.dm,
						video.internation.dashboard.pid.totalflow.dm
                    </reportIndex>
                </dimension>
            </dimensions>
        </report>
    </reports>
</reportSuite>