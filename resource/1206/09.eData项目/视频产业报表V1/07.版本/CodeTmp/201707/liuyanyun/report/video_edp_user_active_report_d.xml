<reportSuite>
    <reports>
        <report tableName = "t_video_develop_user_active_d" 
				keyFields = "projectid,activitychannelid,platformid,dtime" 
				period = "daily" timeFormat = "yyyyMMdd" timeField = "dtime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
				<field name="activitychannelid" regular="[0-9a-zA-Z./-]*"/>
				<field name="platformid" regular="[0-9a-zA-Z./-]*"/>
                <field name="dtime"  regular="[0-9a-zA-Z./-]*"/>
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
                        video.edp.user.active.sdkusers.d,
						video.edp.user.active.totalregisteruser.d,
						video.edp.user.active.allregisteruser.d,
						video.edp.user.active.userstatus.d,
						video.edp.user.active.watchusers.d,
						video.edp.user.active.allproductorder.d
                    </reportIndex>
                </dimension>
                <dimension id = "0" >
                    <reportIndex>
						video.edp.user.active.columns.d
                    </reportIndex>
                </dimension>				
            </dimensions>
        </report>
    </reports>
</reportSuite>
