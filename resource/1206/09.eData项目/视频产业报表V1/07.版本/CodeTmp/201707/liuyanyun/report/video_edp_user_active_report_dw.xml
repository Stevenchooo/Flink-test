<reportSuite>
    <reports>
        <report tableName = "t_video_develop_user_active_sd" 
				keyFields = "projectid,activitychannelid,platformid,dtime" 
				period = "daily" timeFormat = "yyyyMMdd" timeField = "dtime">
            <fieldDefinition>
                <field name="projectid" regular="[0-9a-zA-Z./-]*"/>
				<field name="activitychannelid" regular="[0-9a-zA-Z./-]*"/>
				<field name="platformid" regular="[0-9a-zA-Z./-]*"/>
                <field name="dtime"  regular="[0-9a-zA-Z./-]*"/>
            </fieldDefinition>
            <dimensions>
                <dimension id = "0" >
                    <reportIndex>
                        video.edp.user.active.sdkusers.dw,
						video.edp.user.active.userstatus.dw,
						video.edp.user.active.totalregisteruser.dw,
						video.edp.user.active.watchusers.dw,
						video.edp.user.active.dayactiveusers.dw
                    </reportIndex>
                </dimension>
                <dimension id = "0" >
                    <reportIndex>
						video.edp.user.active.registeruseractiverate.dw
                    </reportIndex>
                </dimension>				
            </dimensions>
        </report>
    </reports>
</reportSuite>
