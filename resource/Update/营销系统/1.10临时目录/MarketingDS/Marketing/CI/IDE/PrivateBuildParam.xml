<?xml version="1.0" encoding="utf-8"?>
<buildCloudParam>
  <baseconfig>
    <property name="productName" value="Markting" />
    <property name="productVersion" value="V600R003C00" />
    <property name="resourceOwnerKey" value="EMUI_BI" />
    <property name="resourceAccessToken" value="EMUI_BI" />
    <property name="isIncreaseBuild" value="true" />
	<!--pbiName为产品版本号，pbiId为对应的数字号 -->
	<property name="pbiName" value="Markting V600R003C00" />
	<property name="pbiId" value="21358102" />	
  </baseconfig>
  <!--定义待依赖的平台，artifactId为自定义的平台id，另外参数为平台的名称，版本-->
  <platforms>
    <platform artifactId="ICP_CI2.1" platformName="ICP_CI" version="2.0" />
  </platforms>
  <advancedconfig>
     <property name="BuildCloudService" value="http://icp-bcc-router3:9080/router/services/RouterService" />
    <!--是否在pom上保留上次的构建结果，默认为true-->
    <property name="isKeepLastResult" value="false" />
	<!--useFullSync只在增量构建下起效，如果useFullSync配置为true，则增量构建时候agent同步代码采用全量同步策略，反之采用增量同步策略，该配置项不区分大小写-->
	<property name="useFullSync" value="true" />
    <!-- 选择所使用的云构建服务地域,与Router上对应 
	shenzhen DNG ;Shanghai SHA; Beijing PEK; Chengdu NCTU; Xi'an XIY ; Wuhan WUH ; Hangzhou HGH; Nanjing NJH;--> 
	<property name="specialLocation" value="DNG" /> 
	<!--回传构建结果以\\10.0.0.1共享目录形式打开 -->
	<property name="sharePomFolder" value="PrivateBuild" /> 
  </advancedconfig>
  <!--定义环境变量，格式如下
  <environmentVariables>
    <environmentVariable name="ant1_4_HOME" value="C:/ant 1.4" />
  </environmentVariables>
  -->
</buildCloudParam>