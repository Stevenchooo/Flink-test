				<param name="mailto" value="z00280396,z00287657,y00220010,c00219980,w00296102,l00166278,l00350030,z00190465,,swx306006,ywx289656,y84030100"/>
				<param name="mailcc" value="z00280396,z00287657,y00220010,c00219980,w00296102,l00166278,l00350030,z00190465,,swx306006,ywx289656,y84030100" />
				<param name="CodeCC.concurrent" value="true" />
				<param name="component" value="" />
				<param name="checkrules" value="coverity,fortify"/>
